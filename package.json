        "build": "cd Client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "dependencies": {
        "moment": "^2.22.2",
        "moment-timezone": "^0.5.21",
        "react-addons-css-transition-group": "^15.6.2",
        "react-infinite-calendar": "^2.3.1",
        "react-moment": "^0.7.9"
    }
}
